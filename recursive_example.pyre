// 0, 1, 1, 2, 3, 5, 8, 13, 21 ...

process fibonacci(int first, int second, int end){
     String func(){
          print(first);
          is (first == 0){ //base case
              fibonacci(1, 1, end);
          }
          is (first > end){ // to prevent infinite
               emit done("complete");
          }

          fibonacci(second, first+second, end);
      }
}

main(){
     fibonacci(first: 0, second: 0, end: 21) -> print(done);
}